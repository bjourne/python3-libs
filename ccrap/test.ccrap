! TODO: understand what "inconsistent dll linkage" means
C: printf0 int printf ( const char* , ... ) 0
C: printf1 int printf ( const char* , ... ) 1
C: puts int puts ( const char* ) 0

: print-int ( i -- )
    "%lld\n" swap printf1 drop ;

: 2drop ( x y -- )
    drop drop ;

: 2dip ( x y quot -- x y )
    swap [ dip ] dip ;

: times ( n quot -- ... )
    over 0 = [ 2drop ] [
        swap over
        [ call ] 2dip
        swap 1 - swap times
    ] if ;

: fib ( n -- n' )
    dup 2 < [ ] [
        dup 1 - fib
        swap 2 - fib +
    ] if ;

: fib-iter ( n -- n' )
    dup 2 < [ ] [
        [ 0 1 ] dip [
            swap dup [ + ] dip
        ] times drop
    ] if ;

: main ( argc argv -- )
    2drop 150 fib-iter print-int ;
